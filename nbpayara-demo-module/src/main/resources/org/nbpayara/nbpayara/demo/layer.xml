<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="nbpayara-demo">
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.nbpayara.nbpayara.demo.Kit"/>
                </file>
                <folder name="Preferences">
                    <folder name="Defaults">
                        <file name="nbpayara-demo-editor-Preferences.xml" url="nbpayara-demo-editor-Preferences.xml"/>
                    </folder>
                </folder>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="org-netbeans-modules-editor-settings-CustomFontsColors-tokenColorings.xml" url="editor-settings-CustomFontsColors-tokenColorings.xml">
                                <attr name="nbeditor-settings-ColoringType" stringvalue="token"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>
